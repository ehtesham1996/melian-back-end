    "fork-ts-checker-webpack-plugin": "^6.0.1",
    "jest": "26.0.1",
    "aws-sdk": "^2.819.0",
    "aws-lambda": "^1.0.6",
    "prettier": "^1.19.1",
    "serverless": "^2.11.1",
    "serverless-dotenv-plugin": "^3.1.0",
    "serverless-offline": "^6.8.0",
    "serverless-prune-plugin": "^1.4.3",
    "serverless-webpack": "^5.3.5",
    "supertest": "^4.0.2",
    "ts-jest": "26.1.0",
    "ts-loader": "^6.2.2",
    "ts-node": "^8.6.2",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^3.7.4",
