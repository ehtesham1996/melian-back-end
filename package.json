    "rxjs": "^6.5.4",
    "source-map": "^0.7.3",
    "source-map-support": "^0.5.19",
    "webpack-graphql-loader": "^1.0.2"
  },
  "devDependencies": {
    "@nestjs/cli": "^7.0.0",
    "@nestjs/schematics": "^7.0.0",
    "@nestjs/testing": "^7.0.0",
    "@types/express": "^4.17.3",
    "@types/jest": "25.2.3",
    "@types/jsonwebtoken": "^8.5.0",
