    "aws-serverless-express": "^3.3.8",
    "copy-webpack-plugin": "^7.0.0",
    "express": "^4.17.1",
    "express-fileupload": "^1.2.0",
    "graphql": "^15.4.0",
    "graphql-scalars": "^1.7.0",
    "graphql-tools": "^7.0.2",
